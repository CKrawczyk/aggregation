[Databse]
database_type = mysql
users_cmd = select id from zooniverse_user;
clicks_cmd = select classifications.zooniverse_user_id, classifications.light_curve_id, where task_id = 4 join classifications on classification_id = classifications.id from annotations;
subjects_md = select id from light_curves;

[Project]
project_type = PlanetHunters

